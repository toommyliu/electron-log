{
  "name": "electron-log",
  "version": "5.4.3",
  "description": "Just a simple logging module for your Electron application",
  "main": "dist/index.js",
  "browser": "dist/renderer/index.js",
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint '**/*.{js,cjs}' --ignore-pattern '**/dist/*.js' && tsc --noEmit",
    "prepare": "npm run build",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint"
  },
  "typings": "src/index.d.ts",
  "repository": "megahertz/electron-log",
  "files": [
    "dist/**",
    "src/**/*.d.ts",
    "!**/.*",
    "main.js",
    "main.d.ts",
    "node.js",
    "node.d.ts",
    "preload.js",
    "renderer.js",
    "renderer.d.ts"
  ],
  "keywords": [
    "electron",
    "atom",
    "log",
    "logger",
    "logging",
    "windows",
    "mac",
    "osx",
    "linux",
    "desktop"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/electron-log/issues",
  "homepage": "https://github.com/megahertz/electron-log#readme",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "electron": "*",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "nw": "^0.100.1",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vite": "^4.5.9",
    "vite-plugin-electron": "^0.15.5",
    "vite-plugin-electron-renderer": "^0.14.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
